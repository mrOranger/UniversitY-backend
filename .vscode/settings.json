{
  "editor.fontFamily": "Martian Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 10,
  "editor.fontWeight": "300",
  "editor.letterSpacing": 0.8,
  "editor.lineHeight": 2.5,
  "editor.autoClosingComments": "always",
  "editor.autoClosingBrackets": "always",
  "editor.codeLensFontSize": 15,
  "editor.cursorBlinking": "expand",
  "editor.cursorStyle": "underline-thin",
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.indentSize": "tabSize",
  "editor.padding.top": 20,
  "editor.padding.bottom": 20,
  "terminal.integrated.fontFamily": "Martian Mono",
  "terminal.integrated.fontSize": 10,
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.letterSpacing": 1,
  "terminal.integrated.lineHeight": 2.5,
  "terminal.integrated.cursorStyle": "line"
}
